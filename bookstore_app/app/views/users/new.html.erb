<div class="container">
<h1> Add New User</h1>
<%= form_for :user, url: users_path do |f| %>
<div class="form-group">
<%= f.label :fullname %>
<%= f.text_field :fullname %>
</div>
<div class="form-group">
<%= f.label :email %>
<%= f.text_field :email %>
</div>
<div class="form-group">
<%= f.label :user_id %>
<%= f.text_field :user_id %>
</div>
<div class="form-group">
<%= f.label :age %>
<%= f.number_field :age %>
</div>
<div class="form-group">
<%= f.submit %>
</div>
<% end %>
</div>