<div class="container">
<h2> Authors Available </h2>
<table border="1" class="table">
<tr>
<th>Author First Name</th>
<th>Author Last Name</th>
<th>Author Bio</th>
</tr>
<% @authors.each do |author| %>
<tr>
<td> <%= author.fname %> </td>
<td> <%= author.lname %> </td>
<td> <%= author.bio %> </td>
<td> <%= link_to 'Author Page', author_path(author) %> </td>
</tr>
<% end %>
</table>
</div>