<div class="container">
<h1> Add New Publisher</h1>
<%= form_for :publisher, url: publishers_path do |f| %>
<div class="form-group">
<%= f.label :name %>
<%= f.text_field :name, class:'form-control' %><br />
</div>
<div class="form-group">
<%= f.label :country %>
<%= f.text_field :country, class:'form-control' %><br />
</div>
<%= f.submit "Submit", class:'btn btn-primary' %>
<%=link_to "Cancel", publishers_path, class:'btn btn-default' %>
<% end %>
</div>